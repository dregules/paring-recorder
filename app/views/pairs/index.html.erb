<%= @time.strftime("%A")%> Week <%= @week_number %>



<%= form_for Pair.new do |f| %>
  <%= f.select(:studentOne) do %>
    <% @available_students.each do |c| %>
      <%= content_tag(:option, c, value: c) %>
    <% end %>
  <% end %>
  <%= f.select(:studentTwo) do %>
    <% @available_students.each do |c| %>
      <%= content_tag(:option, c, value: c) %>
    <% end %>
  <% end %>
  <%= f.submit %>
<% end %>


Pairs chosen today:

<% @pairs.each do |pair| %>
  <br>
  <%= pair.studentOne %> & <%= pair.studentTwo %>
<% end %>
